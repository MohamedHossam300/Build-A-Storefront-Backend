{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "lint": "eslint \"src/**/*.ts\"",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "migrate": "db-migrate --env test up && db-migrate up",
    "dev": "db-migrate reset && db-migrate up",
    "test": "npm run build && set ENV=test&& db-migrate --env test reset && db-migrate --env test up && npm run jasmine && db-migrate --env test reset",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jasmine": "^4.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.8",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
